{
  "name": "react-template",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 8173",
    "dev:host": "vite --host --port 8173",
    "build": "tsc && vite build",
    "build:prod": "tsc && vite build --base=/app",
    "test": "vitest",
    "preview": "vite preview --port 8173",
    "tailwind:watch": "npx tailwindcss -i ./src/styles/global.css -o ./dist/styles/global.css --watch",
    "tailwind:build": "npx tailwindcss -i ./src/styles/global.css -o ./dist/styles/global.css",
    "storybook": "storybook dev -p 9009",
    "storybook:build": "storybook build",
    "sb-tw:build": "npm run tailwind:build && npm run storybook:build",
    "storybook:watch": "concurrently -n \"watch-storybook,watch-tailwind\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run storybook\" \"npm run tailwind:watch\"",
    "chromatic": "env-cmd --file .env.local chromatic --b sb-tw:build --zip --exit-zero-on-changes"
  },
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
