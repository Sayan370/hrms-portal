    "formik": "^2.2.9",
    "framer-motion": "^10.11.2",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "msw": "^1.2.1",
    "pdfmake": "^0.2.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-error-boundary": "^4.0.3",
    "react-helmet": "^6.1.0",
    "react-input-mask": "^3.0.0-alpha.2",
    "react-keyed-flatten-children": "^2.2.1",
    "react-query": "^3.39.3",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "react-snap-carousel": "^0.3.2",
